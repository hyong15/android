<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/seekbar_circle_normal" android:state_enabled="false"/>
    <!-- 滑块未压下时 -->
    <item android:drawable="@drawable/seekbar_circle_normal" android:state_pressed="false"></item>

    <!-- 滑块压下时 -->
    <item android:drawable="@drawable/seekbar_circle_pressed" android:state_pressed="true"></item>
    <item android:drawable="@drawable/seekbar_circle_pressed" android:state_focused="true"/>

</selector>